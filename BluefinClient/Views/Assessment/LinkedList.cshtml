<form novalidate id="linkedListForm" name="linkedListForm">
    <h1>Get 5th element from the end</h1>
    <table>
        <tr>
            <td>
                <h3>{{model.result}}</h3>
            </td>
        </tr>
    </table>
    <div>
        <button type="button" ng-click="submit()" ng-disabled="model.isBusy">Submit</button>
        <button type="button" ng-click="back()" ng-disabled="model.isBusy">Back</button>
        {{model.message}}
    </div>
</form>